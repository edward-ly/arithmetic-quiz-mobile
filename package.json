{
  "name": "arithmetic-quiz-mobile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "jest-expo": "23.0.0",
    "prettier": "1.11.1",
    "react-native-scripts": "1.9.0",
    "react-test-renderer": "16.0.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
