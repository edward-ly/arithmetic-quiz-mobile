  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "expo": "^23.0.4",
    "prop-types": "^15.6.0",
    "react": "16.0.0",
    "react-native": "0.50.3",
    "react-native-modal": "^5.0.0",
    "react-native-root-toast": "^3.0.1",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.0.3"
